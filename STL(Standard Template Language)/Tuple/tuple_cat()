#include<iostream>
#include<tuple>
using namespace std;

int main()
{
    tuple<int, int, int> tuple1;
    tuple<int,string,string> tuple2;
    tuple<int,string,string> tuple3;
    tuple1=make_tuple(1,2,3);
    tuple2=make_tuple(1,"Study","Tonight");
    tuple3=make_tuple(2,"Hard"," work");
         
     //tuple_cat():This function concatenates two tuples and returns a new tuple
     
     auto tuple4=tuple_cat(tuple2,tuple3);
     cout<<get<0>(tuple4)<<" "<<get<1>(tuple4)<<" "<<get<2>(tuple4)<<" "<<get<3>(tuple4)<<" "<<get<4>(tuple4)<<" "<<get<5>(tuple4);
    
    
    return 0;
}
